// Test of BFS
Proc b2_bfs(G:Graph, A,B: N_P<Int>(G), k:Node(G))
{
    Int z = 0;
    For(s:G.Nodes) (s.B > 0)
    {   
        InBFS(t: G.Nodes ; s)
        {
            Int y = s.B;
            Foreach(u: t.UpNbrs) {
                t.A += u.B @ u;
                z+= y;
            }
        }
    }

    InBFS(s:G.Nodes From k)[s.B > 1]
    {}
    InReverse
    {
        z += s.A;
    }
}
